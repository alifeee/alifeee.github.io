{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": ["DOM", "ES6", "ScriptHost", "ES2015.Promise"],
    "declaration": false,
    "sourceMap": false,
    "outDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": true
  },
  "exclude": [
    "node_modules",
    "bower_components",
    "jspm_packages",
    "tmp",
    "temp"
  ]
}
